    "classnames": "^2.2.6",
    "css-loader": "^3.4.2",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^4.0.4",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "node-sass": "^4.13.1",
    "popper.js": "^1.15.0",
    "react": "^16.13.1",
